{
  "name": "JoshsFrogsAdmin",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "electron:rebuild-deps-windows": "./node_modules/.bin/electron-rebuild.cmd -m ./node_modules/capacitor-mettler-toledo && ./node_modules/.bin/electron-rebuild.cmd -m ./node_modules/capacitor-zebra-printer",
    "electron:rebuild-deps-linux": "$(npm bin)/electron-rebuild -m ./node_modules/capacitor-mettler-toledo && $(npm bin)/electron-rebuild -m ./node_modules/capacitor-zebra-printer",
    "electron:start": "npm run build && electron ./",
    "electron:pack": "npm run build && electron-builder build --dir",
    "electron:build-windows": "npm run build && electron-builder build --windows",
    "electron:build-mac": "npm run build && electron-builder build --mac",
    "electron:package": "electron-packager . --overwrite --asar --icon=src/assets/favicon.ico",
    "electron:package-no-asar": "electron-packager . --overwrite --icon=src/assets/favicon.ico"
  },
  "dependencies": {
    "@capacitor-community/electron": "1.3.0-1",
    "@capacitor/core": "^2.4.2",
    "capacitor-machine-id": "file:../capacitor-machine-id",
    "capacitor-mettler-toledo": "file:../capacitor-mettler-toledo"
  },
  "devDependencies": {
    "electron": "^10.1.4",
    "electron-builder": "^22.8.1",
    "electron-rebuild": "^2.3.5",
    "typescript": "^4.0.7"
  },
  "keywords": [
    "capacitor",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "com.joshsfrogs.admin",
    "productName": "Josh's Frogs Admin",
    "files": [
      "assets/*",
      "build/*",
      "preloader.js",
      "plugins/*",
      "capacitor.config.json",
      "app/**"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "appIcon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "appIcon.ico"
    }
  }
}
